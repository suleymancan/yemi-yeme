{
    "name":"Yemi Yeme - Clickbait Dedektörü",
    "version": "0.0.1",
    "manifest_version": 2,
    "author":"Süleyman Can",
    "description": "Sosyal medya clickbait dedektörü!",
    "permissions": [
        "webRequest",
        "<all_urls>"
    ],
    "icons": {
        "16": "yemi-yeme.png",
        "48": "yemi-yeme.png",
        "128": "yemi-yeme.png" },

    "homepage_url": "https://github.com/suleymancan/yemi-yeme",
    "content_scripts": [ 
        {
            "matches":[
                "*://*.twitter.com/*"
            ],
            "js": ["twitter.js"]
        }
    ]

    
}